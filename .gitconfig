[core]
	editor = nvim
	excludesfile = ~/.gitignore
	commitGraph = true
	hooksPath = ~/.git-hooks
[color]
	ui = true
[gpg]
	program = gpg2
[commit]
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
